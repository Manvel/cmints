<% if (items) { %>
  <% if (items.length) { %>
  <ul>
  <% } %>
      <% items.forEach(function(item){ %>
      <li>
          <% if (item.path) { %>
            <a <%-href(item.path)%>><%= item.file %></a>
          <% } else {  %>
            <%= item.file %>
          <% } %>
          <%- include('navigation', {items: item.children}) %>
      </li>
      <% }) %>
  <% if (items.length) { %>
  </ul>
  <% } %>
<% } %>
