<% items.forEach(function(item){ %>
  <li>
    <% let title = ""; %>
    <% if (item.metadata && item.metadata.title) { %>
      <% title = item.metadata.title; %>
    <% } %>
    <% if (item.title) { %>
      <% title = item.title; %>
    <% } %>
    <% if (item.url) { %>
      <a <%-href(item.url)%>
        <% if (item.url == currentPage) { %>class="active"<% } %>>
        <%= title %>
      </a>
    <% } else { %>
      <%= title %>
    <% } %>
  </li>
<% }) %>
